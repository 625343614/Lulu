@charset "utf-8";
/*css reset**/
*{
	box-sizing: border-box;
}
body{
	margin:0;
	font-family: "Helvetica Neue",Helvetica,Arial,"Hiragino Sans GB",微软雅黑,tahoma,simsun,宋体;
	font-size: 12px;
	line-height: 24px;
	color: #333;
}
ul,li.dl,ol,h1,h2,h3,h4,h5,h6,p,div{
	margin:0;
	padding:0;
}
ul,ol,dl{list-style: none;}
a{
	color: #333;
	text-decoration: none;
}
a:hover{
	color: #e31436;
	text-decoration: none;
}
.cleatfloat:after{
	content: ".";
	height: 0;
	clear: both;
	visibility: hidden;
}
.clearfloat{zoom:1;}
.lf{float: left;}
.rt{float: right;}
.container,#top_main,#nav,#banner,#main{
	width: 1090px;
	margin:0 auto;
}
input{
	outline:none;
	border:0;
}
/*********top_box 32px 1090px*************/
#top_box{
	width: 100%;
	height:30px;
	background: #f2f2f2;
	line-height: 30px;
}
.login{
	margin-left: 16px;
}
.sep_line{
	padding:0 10px;
	color: #ccc;
}
.phone_kaola{
	position: relative;
}
.phone_kaola a{
	float: left;
	margin-left: 30px;
}

.sjweixin{
	width: 105px;
	height: 142px;
	position: absolute;
	top:34px;
	left: 200px;
	z-index: 10;
	display: none;
}
.tubiao{
	float: left;
	width: 9px;
	height: 12px;
	background: url(../images/icon.png) no-repeat;
	margin:8px 6px;	
}
#top_box .sep_line{
	padding:0;
}
#top_box ul.rt li{
	float: left;
	text-align: center;
	position: relative;
	
}
#top_box ul.rt li a{
	float: left;
	padding:0 20px 0 8px;
}
#top_box .rt .li_1 i{
	background-position: -14px 0;
}
#top_box .rt .li_5 i{
	width: 12px;
	background-position: -27px 0;
}
#top_box .rt b{
	border-right:4px solid transparent;
	border-left:4px solid transparent;
	border-bottom:4px solid #666;
	transition: all .2s;
	position: absolute;
	top:12px;
	right:12px;
}
.li_3:hover,.li_4:hover,.li_7:hover{
	background: #fff;
}
.li_3:hover span,.li_4:hover span,.li_7:hover span{
	color: #fff;
}
#top_box .li_3:hover b,#top_box .li_4:hover b,#top_box .li_7:hover b{
	transform:rotate(180deg);
	-ms-transform:rotate(180deg);       /*ie9*/
	-webkit-transform:rotate(180deg);   /*Safari and Chrome*/
	-o-transform:rotate(180deg);        /*opera*/
	-moz-transform:rotate(180deg);      /*Firefox*/
	
}
.sup_li_3 {
	background: #fff;
	position: absolute;
	top: 30px;
	left: 0px;
	padding:10px 0 10px 5px;
	box-shadow: 0 1px 1px rgba(0,0,0,0.12);
	z-index: 10;
	display: none;
}
.sup_li_3 a{
	width: 100px;
	display: block;
	text-align: left;
}
.sup_li_4{
	position: absolute;
	top: 30px;
	left: 0px;
	background: #fff;
	z-index: 10;
	width: 94px;
	box-shadow: 0 1px 1px rgba(0,0,0,0.12);
	display: none;
}
.sub_li_4 a{
	display: block;
	padding-left: 5px;	
}
.sub_li_4 a i{
	display: inline-block;
	background:url(../images/icon1.png) no-repeat;
	vertical-align: middle; 
}
.sub_li_4 .kf i{
	width: 12px;
	height: 14px;
	background-position: -6px -290px;
}
.sub_li_4 .bz i{
	width: 14px;
	height: 14px;
	background-position: -6px -324px;
}
#sup_li_7{
	position: absolute;
	top: 30px;
	right: 0px;
	background: #fff;
	width: 100px;
	z-index: 10;
	padding:4px 15px 12px 14px;
	box-shadow: 0 1px 1px rgba(0,0,0,0.12);
	display: none;
}
#top_box  #sup_li_7 a{
	display: block;	
	padding:0 10px;
}
 #sup_li_7 .weibo{
	border-top:1px solid red;
}
/*******top_main***********/
#top_main{
	height: 100px;
	position: relative;
}
.search_box{
	float: left;
	width: 513px;
	margin:27px 80px 0 300px;
}
#top_main>a{
	float: left;
	position: absolute;
	top: 29px;
	left: 0px;
}
.search{
	height: 36px;
	background: #333;
	position: relative;
}
#txtSearch{
	float: left;
	width: 460px;
	height: 31px;
	margin: 3px 0 2px 3px ;
	padding-left: 30px;
	color: #aaa;
}
.search .left{
	position: absolute;
	top:12px;
	left:12px;
	width: 15px;
	height: 16px;
	background: url(../images/icon1.png) no-repeat -468px -668px;
}
.search .rt{
	width: 50px;
	height: 36px;
}
.search .rt i{
	float: right;
	width: 20px;
	height: 20px;
	background: url(../images/icon1.png) no-repeat -505px -665px;
	margin-right: 14px;
	margin-top: 8px;
}
.hot_words .sep_line{
	padding:0 4px;
}
.hot_words a{
	color: #aaa;
}
#shop_box{
	float: right;
	margin-top: 28px;
	text-align:center;
}
#top_main .shop_car{
	float: right;
	width: 108px;
	height: 34px;
	line-height: 34px;
	vertical-align: middle;
	background: #fff;
	font-size: 13px;
	border:1px solid #ddd;
	border-radius:3px;
}
.shop_car i{
	display: inline-block;
	vertical-align: middle;
	width: 19px;
	height: 16px;
	background: url(../images/icon1.png) no-repeat -960px -212px;
}
#top_main .shop_car:hover{
	background: #eee;
	color: #333;
}
/************nav**************/
#nav{
	width: 100%;
	height: 40px;
	border-bottom: 2px solid #151515;
	background: #fff;
}
.sort{
	width: 160px;
	height: 40px;
	line-height: 40px;
	background: #151515;
	color: #fff;
	font-size: 14px;
	text-align: left;
	padding-left: 28px;
}
.sort_line{
	display: inline-block;
	vertical-align: middle;
	padding:0 10px 0 0;
}
.sort_line i{
	display: block;
	width: 15px;
	height: 0px;
	border:1.5px solid #ddd;
	margin-bottom: 3px;
}
#top_caps{
	float: left;
}
#sort_box{
	position: absolute;
	text-align: center;
	z-index: 99;
}
#sort_box li{
	width: 160px;
	height: 40px;
	background: rgba(15,15,15,.82);
	text-align: center;
	color: #fff;
	font-size: 14px;
	line-height: 40px;
}
#sort_box li>img{
	vertical-align: middle;
	margin-right: 5px;
}
#sort_box li span{
	margin-right: 15px;
	padding-left: 5px;
}
#sort_box li .tubiao1{
	display: inline-block;
	width: 5px;
	height: 10px;
	background: url(../images/icon1.png) no-repeat -9px -100px;
	vertical-align: middle;
}
#sort_box li em{
	display: block;
	position: relative;
	bottom: 1px;
	left: 8px;
	width: 140px;
	height: 1px;
	background: rgba(255,255,255,.05);
}
.nav_right{margin-left: 15px;}
.nav_right li{
	float: left;
	line-height: 40px;
	font-size: 16px;
	color: #151515;
	font-weight: bold;
	padding:0 20px;
}
/*******轮播图片*************/
#banner{
	position: relative;
	width: 100%;
	height: 400px;
}
#banner img{
	position: absolute;
	top: 0px;
	left: -165px;
	width: 1920px;
	height: 400px;
}
/*banner的前后按钮**/
.cons{
	background: url(../images/icon1.png) no-repeat;
}
#control>div{
	width: 940px;
	margin:0 auto;
	position: absolute;
	top: 50%;
	left: 25%;
	margin-top: -34px;
}
#control>div a{
	width:34px;
	height: 68px;
}
.prev{
	float: left;
	background-position: -627px -68px;
}
.next{
	float: right;
	background-position: -670px -67px;
}
.prev:hover{
	background-position: -627px -153px;
}
.next:hover{
	background-position: -671px -153px;
}
#page_box{
	position: absolute;
	bottom: 0px;
	left: 50%;
}
#page_box a{
	float:left;
	position: relative;
	width: 20px;
	height: 20px;
	margin:0 4px 20px 0;
}
#page_box i{
	display: inline-block;
	width: 20px;
	height: 20px;
	background: rgba(0,0,0,.5);
	border-radius: 50%;
}
#page_box span{
	color: #fff;
	font-size: 13px;
	line-height: 20px;
	text-align: center;
	position:absolute;
	top: 0px;
	left: 6px;
}
#page_box a:hover i{
	background: #e31436;
}
/******产品的弹出导航***********/
.sup_card{
	float: left;
	width: 930px;
	height: 400px;
	background: #fff;
	position: absolute;
	top: 0px;
	left: 160px;
	padding-left: 20px;
	border:1px solid #000;
}
.card_box{
	width: 100%;
}
.card_lf{
	float: left;
	width: 610px;
	overflow: hidden;
}
.lf_top{
	overflow: hidden;
}
.lf_top,.card_lf{
	text-align: left;
	color: #333;
	cursor: auto;
}
.litd{
	float: left;
	padding-top: 5px;
}
.item{
	display: inline-block;
	padding: 0;
	margin: 0 25px 10px 5px;
	width: 275px;
	height: auto;
	vertical-align: top;
	line-height: normal;
	overflow: hidden;
}
.item p{
	height: 34px;
	line-height: 34px;
	font-size: 14px;
	font-weight: bold;
	border-bottom: 1px solid #eaeaea;
}
.item p a:hover{
	text-decoration: underline;
	color: #333;
}
.namebox{
	margin-left: -8px;
}
#sort_box .namebox a{
	display: inline-block;
	height: 14px;
	line-height: 14px;
	padding: 0 5px 0 9px;
	color: #666;
	text-align: left;
	border-left: 1px solid #ccc;
	font-size: 13px;
	margin-top: 12px;
}
.namebox a:hover{
	text-decoration: underline;
}
.card_rt{
	width: 280px;
	height: 398px;
	float: right;
	background: #f6f6f6;
	padding: 20px;
}
.imgs{
	width: 243px;
	height: 243px;
	overflow: hidden;
}
.imgs a{
	float: left;
}
.imgs img{
	display: inline-block;
	width: 80px;
	height: 80px;
	vertical-align: middle;
	border:1px solid #eaeaea;
	margin: -1px 0 0 -1px;
}
.img_bottom{
	padding-top:15px;
}
.article{
	width: 100%;
	height: 44px;
	border-bottom: 1px solid #eaeaea;
}
.article .container{
	line-height: 44px;
}
.article .container i{
	display: inline-block;
	width: 40px;
	height: 40px;
	background: url(../images/slogan.png) no-repeat;
}
.article .container .tuihuo{
	background-position: 0 0;
}
.article .container .tuihuo{
	background-position: -75px 0;
}
.article .container .baoyou{
	background-position: -150px 0;
}